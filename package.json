  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "start": "node --exec tsx src/server.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "db:migrate:undo-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo",
    "db:migrate:undo:all-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "db:migrate:status-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize db:migrate:status",
    "db:seed-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize db:seed",
    "db:seed:all-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "db:seed:undo-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize db:seed:undo",
    "db:seed:undo:all-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize db:seed:undo:all",
    "db:migration:generate-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize migration:generate --name",
    "db:seed:generate-esm": "node --loader ts-node/esm ./node_modules/sequelize-cli/lib/sequelize seed:generate --name",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:migrate:status": "sequelize db:migrate:status",
    "db:seed": "sequelize db:seed",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "db:migration:generate": "sequelize migration:generate --name",
    "db:seed:generate": "sequelize seed:generate --name"
  },
